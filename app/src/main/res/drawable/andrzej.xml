<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="64dp"
        android:height="64dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/gray" />
            <solid android:color="@color/transparent" />
